#!/usr/bin/python
# -*- coding: utf-8 -*-

import os
import sys

if __name__ == "__main__":
    sys.path.insert( 0,  os.path.join(os.sep, *os.path.realpath(__file__).split(os.sep)[:-2]) )

    try:
        import pybaseproject.__main__
    except:
        print "cannot import pybaseproject.__main__"
        sys.exit(1)
    pybaseproject
